{
  "name": "frontendv2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "npm run lint:tsc && npm run lint:eslint && npm run lint:prettier",
    "lint:tsc": "tsc --noEmit",
    "lint:eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:prettier": "prettier --check .",
    "fix": "npm run fix:eslint && npm run fix:prettier",
    "fix:eslint": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
    "fix:prettier": "prettier --write .",
    "lint:conflict": "eslint-config-prettier .eslintrc.json",
    "postinstall": "typesync",
    "check": "typesync && npm outdated && npm audit && npm run lint:conflict && npm run lint && npm run dev"
  },
  "dependencies": {
    "@apollo/client": "^3.3.16",
    "@apollo/react-hooks": "^4.0.0",
    "@hookform/resolvers": "^2.4.0",
    "constate": "^3.2.0",
    "graphql": "^15.5.0",
    "next": "^11.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.4.0",
    "react-pose": "^4.0.10",
    "styled-components": "^5.3.0",
    "subscriptions-transport-ws": "^0.9.18",
    "uniqid": "^5.3.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/eslint": "^7.2.10",
    "@types/fontfaceobserver": "^0.0.6",
    "@types/node": "^15.3.0",
    "@types/prettier": "^2.2.3",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "@types/styled-components": "^5.1.9",
    "@types/uniqid": "^5.3.0",
    "@types/yup": "^0.29.11",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "autoprefixer": "^10.2.5",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.25.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "2.2.1",
    "typescript": "^4.3.2",
    "typesync": "^0.8.0"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 120
  }
}
